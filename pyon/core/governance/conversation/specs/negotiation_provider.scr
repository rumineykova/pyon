protocol Negotiation at provider(role requester, role provider, role adviser)
{
    propose(string) from requester;
    (string) to requester;
    rec Start{
        choice at requester {
            reject(string) from requester;
            () to requester;
        } or {
            accept(string) from requester;
            () to requester;
        } or {
            propose(string) from requester;
            (string) to requester;
            Start;}
            }
}