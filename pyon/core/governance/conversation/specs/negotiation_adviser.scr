protocol Negotiation at adviser(role requester, role provider, role adviser)
{
    consult(string) from requester;
    (string) to requester;
    rec Start{
            choice at requester {
                accept(string) from requester;
                () to requester;
            } or {
                reject(string) from requester;
                () to requester;

            } or {
                consult(string) from requester;
                (string) to requester;
                Start;}
                }
}s